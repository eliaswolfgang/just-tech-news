  "dependencies": {
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "graphql": "^16.6.0",
    "if-env": "^1.0.4",
    "mysql2": "^3.14.2",
